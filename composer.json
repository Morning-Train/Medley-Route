{
    "name": "morningmedley/route",
    "description": "A Laravel-like router for the WordPress Rewrite API",
    "keywords": [
        "wordpress",
        "morningtrain",
        "route"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mathias Munk",
            "email": "mh@morningtrain.dk"
        }
    ],
    "autoload": {
        "psr-4": {
            "MorningMedley\\Route\\": "src/",
            "MorningMedley\\Facades\\": "facades/"
        }
    },
    "require": {
        "morningmedley/application": "^0.3",
        "illuminate/pipeline": "^12.3",
        "illuminate/routing": "^12.3",
        "symfony/finder": "^v7"
    },
    "extra": {
        "morningmedley": {
            "providers": [
                "MorningMedley\\Route\\RoutingServiceProvider",
                "MorningMedley\\Route\\RouteServiceProvider"
            ]
        }
    }
}
